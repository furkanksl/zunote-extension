{
  "name": "ZU-NOTE",
  "description": "Take a note quickly!",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": ["webRequest"],
  "host_permissions": ["<all_urls>"],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_title": "ZU-NOTE",
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon.png",
      "32": "icon.png"
    }
  }
}